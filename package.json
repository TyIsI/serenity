        "@fortawesome/free-regular-svg-icons": "6.6.0",
        "@fortawesome/free-solid-svg-icons": "6.6.0",
        "@fortawesome/react-fontawesome": "0.2.2",
        "@headlessui/react": "2.2.0",
        "@heroicons/react": "2.1.5",
        "@next/bundle-analyzer": "15.0.3",
        "clsx": "2.1.1",
        "debug": "4.3.7",
        "next": "15.0.3",
        "pretty-state-machine": "0.0.10",
        "react": "18.3.1",
        "react-beautiful-dnd": "13.1.1",
        "react-dom": "18.3.1",
        "react-geolocated": "^4.2.0",
        "react-interval-hook": "^1.1.4",
        "react-markdown": "9.0.1",
        "unsplash-js": "7.0.19",
        "wireit": "0.14.9",
        "zod": "3.23.8"
    },
    "devDependencies": {
        "@babel/eslint-parser": "7.25.9",
        "@chromatic-com/storybook": "3.2.2",
        "@next/eslint-plugin-next": "^15.0.3",
        "@storybook/addon-essentials": "8.4.4",
        "@storybook/addon-interactions": "8.4.4",
        "@storybook/addon-links": "8.4.4",
        "@storybook/addon-onboarding": "8.4.4",
        "@storybook/blocks": "8.4.4",
        "@storybook/nextjs": "8.4.4",
        "@storybook/react": "8.4.4",
        "@storybook/test": "8.4.4",
        "@testing-library/react": "16.0.1",
        "@tyisi/config-changelog": "^1.0.1",
        "@tyisi/config-eslint": "4.0.0",
        "@tyisi/config-prettier": "1.0.1",
        "@tyisi/config-stylelint": "1.1.0",
        "@types/debug": "4.1.12",
        "@types/jest": "29.5.14",
        "@types/node": "20.17.6",
        "@types/react": "18.3.12",
        "@types/react-dom": "18.3.1",
        "autoprefixer": "10.4.20",
        "eslint": "9.14.0",
        "eslint-config-next": "15.0.3",
        "eslint-plugin-jest": "28.9.0",
        "eslint-plugin-storybook": "0.11.0",
        "generate-react-cli": "8.4.9",
        "globals": "^15.12.0",
        "jest": "^29.7.0",
        "postcss": "8.4.47",
        "prettier": "3.3.3",
        "sass": "1.80.6",
        "storybook": "8.4.2",
        "stylelint": "16.10.0",
        "swr": "2.2.5",
        "tailwindcss": "3.4.14",
        "typescript": "4.9.5"
    },
    "resolutions": {
        "strip-ansi": "6.0.1",
        "string-width": "4.2.2",
        "wrap-ansi": "7.0.0"
    },
    "scripts": {
        "build": "wireit",
        "build:storybook": "wireit",
        "commit": "npx git-cz@latest",
        "create:component": "generate-react c",
        "create:widget": "generate-react c --type=widget",
        "dev": "wireit",
        "full-dev": "wireit",
        "lint": "wireit",
        "push": "wireit",
        "release": "wireit",
        "start": "wireit",
        "storybook": "wireit",
        "type-check": "wireit",
        "preinstall": "npx only-allow pnpm"
    },
    "wireit": {
        "build": {
            "dependencies": [
                "lint"
            ],
            "command": "next build"
        },
        "build:storybook": {
            "command": "storybook build"
        },
        "dev": {
            "command": "next dev --hostname 127.0.0.1 --port 7001 --experimental-https",
            "service": true
        },
        "full-dev": {
            "dependencies": [
                "dev",
                "storybook"
            ]
        },
        "lint": {
            "command": "next lint"
        },
        "push": {
            "dependencies": [
                "build"
            ],
            "command": "git push"
        },
        "release": {
            "dependencies": [
                "release:version",
                "release:push"
            ]
        },
        "release:push": {
            "dependencies": [
                "release:version"
            ],
            "command": "git push --follow-tags origin main"
        },
        "release:version": {
            "command": "npx standard-version"
        },
        "start": {
            "command": "next start",
            "service": true
        },
        "storybook": {
            "command": "storybook dev -p 6006",
            "service": true
        },
        "type-check": {
            "command": "tsc"
        }
    },
    "packageManager": "pnpm@9.13.1+sha512.5ac37545851c55ec58521b6c285db6f848f93ca70889beb4145f5b40fe234e4ea4faee4dad12ac5c139e494374c65d9c3a6ec9a25954378fa286a250e68d2f20"
}
